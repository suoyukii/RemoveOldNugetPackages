<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net9.0</TargetFramework>
        <LangVersion>latestmajor</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <PublishAot>true</PublishAot>
        <Authors>suoyukii</Authors>
        <Copyright>Copyright © 2024 suoyukii</Copyright>
        <PackageProjectUrl>https://github.com/suoyukii/RemoveOldNugetPackages</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/suoyukii/RemoveOldNugetPackages?tab=MIT-1-ov-file</PackageLicenseUrl>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <DebugType>none</DebugType>
    </PropertyGroup>
</Project>
